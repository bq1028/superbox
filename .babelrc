{
  presets: [
    [ 'env', { loose: true } ],
    'stage-0',
    'react'
  ],
  env: {
    emotion: {
      plugins: [
        [
          'transform-rename-import',
          {
            replacements: [
              {
                original: '^styled-components$',
                replacement: 'react-emotion'
              },
              {
                original: './theming/styled-components',
                replacement: './theming/emotion'
              },
            ]
          }
        ]
      ]
    }
  }
}
